{
  "name": "stylelint-config-mercari",
  "version": "0.0.1",
  "description": "A setting file for stylelint.",
  "main": "index.js",
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "nyc ava",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kouzoh/stylelint-config-mercari.git"
  },
  "keywords": [
    "stylelint",
    "stylelint-config"
  ],
  "author": "abouthiroppy <git@about-hiroppy.com> (http://about-hiroppy.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kouzoh/stylelint-config-mercari/issues"
  },
  "homepage": "https://github.com/kouzoh/stylelint-config-mercari#readme",
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "conventional-changelog-cli": "^1.3.1",
    "eslint": "^3.19.0",
    "nyc": "^11.0.2",
    "stylelint": "^7.10.1"
  }
