{
  "name": "@mercari/stylelint-config",
  "version": "0.0.3",
  "description": "A setting file for stylelint.",
  "main": "index.js",
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "nyc ava",
    "lint": "eslint ."
  },
  "publishConfig": {
    "registry": "https://mercari.jfrog.io/mercari/api/npm/npm-universal/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kouzoh/mercari-stylelint-config.git"
  },
  "keywords": [
    "stylelint",
    "stylelint-config"
  ],
  "author": "abouthiroppy <git@about-hiroppy.com> (http://about-hiroppy.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kouzoh/mercari-stylelint-config/issues"
  },
  "homepage": "https://github.com/kouzoh/mercari-stylelint-config#readme",
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true
  },
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "ava": "^1.0.0-beta.6",
    "conventional-changelog-cli": "^1.3.1",
    "eslint": "^3.19.0",
    "nyc": "^11.0.2",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1"
  },
  "peerDependencies": {
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1"
  }
}
